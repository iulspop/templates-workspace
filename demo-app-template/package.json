{
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "@epic-web/client-hints": "^1.3.8",
    "@epic-web/invariant": "^1.0.0",
    "@prisma/adapter-better-sqlite3": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "@react-router/node": "7.13.0",
    "@react-router/serve": "7.13.0",
    "@tabler/icons-react": "^3.36.1",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.3.1",
    "isbot": "^5.1.35",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "7.13.0",
    "tailwind-merge": "^3.4.1",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@biomejs/biome": "^2.4.0",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@playwright/test": "^1.58.2",
    "@react-router/dev": "7.13.0",
    "@storybook/react-vite": "^10.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "happy-dom": "^20.6.1",
    "lefthook": "^2.1.1",
    "prisma": "^7.4.0",
    "storybook": "^10.2.8",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "name": "demo-app-template-sqlite",
  "packageManager": "pnpm@10.29.3",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "better-sqlite3",
      "esbuild",
      "lefthook",
      "prisma"
    ]
  },
  "private": true,
  "scripts": {
    "build": "prisma generate && react-router build",
    "check": "biome check --write .",
    "db:migrate": "prisma migrate dev --name",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force && prisma db push && pnpm db:seed",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "dev": "react-router dev",
    "lint": "biome ci --css-parse-tailwind-directives=true .",
    "prepare": "lefthook install",
    "start": "react-router-serve ./build/server/index.js",
    "storybook": "STORYBOOK=true storybook dev -p 6006",
    "storybook:build": "STORYBOOK=true storybook build",
    "test": "vitest --reporter=verbose --run",
    "test:e2e": "pnpm exec playwright test",
    "test:e2e:ui": "pnpm exec playwright test --ui",
    "test:watch": "vitest --reporter=verbose",
    "typecheck": "prisma generate && react-router typegen && tsc"
  },
  "type": "module"
}
