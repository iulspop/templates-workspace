  "name": "personal-app-template-sqlite-fly-io",
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "better-sqlite3",
      "esbuild",
      "lefthook",
      "prisma",
      "sharp"
    ]
  },
  "private": true,
  "scripts": {
    "build": "prisma generate && react-router build",
    "check": "biome check --write .",
    "db:migrate": "prisma migrate dev --name",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force && prisma db push && pnpm db:seed",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "dev": "NODE_OPTIONS='--import ./instrument.server.mjs' react-router dev",
    "lint": "biome ci --css-parse-tailwind-directives=true .",
    "prepare": "lefthook install",
    "start": "NODE_OPTIONS='--import ./instrument.server.mjs' react-router-serve ./build/server/index.js",
    "storybook": "STORYBOOK=true storybook dev -p 6006",
    "storybook:build": "STORYBOOK=true storybook build",
    "test": "vitest run",
    "test:e2e": "pnpm exec playwright test",
    "test:e2e:ui": "pnpm exec playwright test --ui",
    "test:watch": "vitest",
    "typecheck": "prisma generate && react-router typegen && tsc"
  },
  "type": "module"
}
